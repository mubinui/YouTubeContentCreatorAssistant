# Google ADK Configuration - Example File
# Copy this to .env and fill in your actual values

# Model Provider Configuration
MODEL_PROVIDER=openrouter  # Options: 'openrouter' or 'google'

# OpenRouter Configuration (for GPT OSS 20B)
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_MODEL=openai/gpt-oss-20b  # Available: openai/gpt-oss-20b, openai/gpt-oss-20b:free, openai/gpt-oss-120b
APP_NAME=YouTube Shorts Creator
SITE_URL=https://github.com/mubinui/YouTubeContentCreatorAssistant

# Google Gemini Configuration (fallback)
GOOGLE_API_KEY=your_google_api_key_here
MODEL_NAME=gemini-2.5-flash

# Shared Model Configuration
MAX_TOKENS=8192
TEMPERATURE=0.1

# Project Settings
PROJECT_NAME=GoogleADK
ENVIRONMENT=development

# Optional: Google Cloud Project ID (if using Google Cloud)
GOOGLE_CLOUD_PROJECT_ID=your_project_id_here

# Logging
LOG_LEVEL=INFO
